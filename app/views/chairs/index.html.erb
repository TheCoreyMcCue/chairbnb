
<div class="container-fluid">


  <div class="container-fluid">

    <h1 class="mt-5">Available Chairs near you </h1>
    <div class="chairs-wrapper row">
      <div class="scroll col-12 col-md-8">
        <% @chairs.each do |chair| %>
        <div class="card-product mt-3">
          <%= cl_image_tag chair.photo.key, crop: :fit %>
          <div class="card-product-infos">
            <h2><%= link_to chair.name, chair_path(chair) %></h2>
            <p> <%= chair.description   %> </p>
            <p> â‚¬<%= chair.price %> listing posted by <%= chair.user.email  %></p>
          </div>
        </div>
        <% end %>
      </div>
      <div class="map col-12 col-md-4">

        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d155959.15243058334!2d4.763877632981139!3d52.354792535815676!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c63fb5949a7755%3A0x6600fd4cb7c0af8d!2sAmsterdam!5e0!3m2!1sen!2snl!4v1614180509723!5m2!1sen!2snl" width="100%" height="600" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
      </div>

  </div>
</div>
